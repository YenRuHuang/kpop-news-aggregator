{
  "name": "kpop-news-aggregator",
  "version": "1.0.0",
  "description": "Kpop news aggregation and organization website",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "echo 'Build completed'",
    "start": "node index.js",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "deploy": "node deploy-to-zeabur.js"
  },
  "keywords": ["kpop", "news", "aggregator", "react", "typescript"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}